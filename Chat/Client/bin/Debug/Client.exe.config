<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<system.serviceModel>
		<bindings>
			<wsDualHttpBinding>
				<binding name="WSDualHttpBinding_IChatService" >
					<security mode="None" />
				</binding>
			</wsDualHttpBinding>
		</bindings>
		<client>
			<endpoint address="http://192.168.13.42:9090/ChatService" binding="wsDualHttpBinding"
                bindingConfiguration="WSDualHttpBinding_IChatService" contract="Proxy.IChatService"
                name="WSDualHttpBinding_IChatService">
			</endpoint>
		</client>
	</system.serviceModel>
</configuration>



































