<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<system.serviceModel>
		<bindings>
			<wsDualHttpBinding>
				<binding name="WSDualHttpBinding_IChatService">
					<security mode="None" />
				</binding>
			</wsDualHttpBinding>
		</bindings>
		<services>
			<service name ="Host.ChatService">
				<endpoint address=""
						  binding ="wsDualHttpBinding"
						  contract="Host.IChatService"
						  bindingConfiguration="WSDualHttpBinding_IChatService">

				</endpoint>
				<host>
					<baseAddresses>
						<add baseAddress="http://94.155.124.58:9090/ChatService"/>
					</baseAddresses>
				</host>
			</service>
		</services>
		<behaviors>
			<serviceBehaviors>
				<behavior>
					<serviceMetadata httpGetEnabled="true"/>
				</behavior>
			</serviceBehaviors>
		</behaviors>
	</system.serviceModel>
</configuration>

